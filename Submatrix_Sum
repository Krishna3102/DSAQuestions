//sum of all submatrices of a matrix
//time complexity O(n^2)
//mathematical approach
#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int a[10][10];
    for(int k=0;k<n;k++){
        for(int l=0;l<n;l++){
            cin>>a[k][l];
        }
    }
    int sum=0;
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            sum=sum + (a[i][j] * ((i+1)*(j+1)*(n-i)*(n-j)));
        }
    }
    cout<<sum<<endl;
    return 0;
}
